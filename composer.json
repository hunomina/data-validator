{
  "name": "hunomina/data-validator",
  "license": "proprietary",
  "description": "Library for json schemas validation",
  "type": "library",
  "authors": [
    {
      "name": "hunomina",
      "email": "hugo.f972@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "infection/infection": "^0.26.16"
  },
  "autoload": {
    "psr-4": {
      "hunomina\\DataValidator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "hunomina\\DataValidator\\Test\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
